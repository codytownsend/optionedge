# API Keys
TRADIER_API_KEY=your_tradier_api_key_here
TRADIER_API_URL=https://api.tradier.com
YAHOO_RAPID_API_KEY=your_rapidapi_key_here
FRED_API_KEY=your_fred_api_key_here
QUIVER_API_KEY=your_quiver_api_key_here

# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO

# Cache Settings
CACHE_TTL_SECONDS=300
CACHE_MAX_SIZE=1000

# Rate Limiting
API_RATE_LIMIT_PER_MINUTE=60
API_RETRY_ATTEMPTS=3
API_RETRY_DELAY_SECONDS=1

# Database (if using one)
DATABASE_URL=sqlite:///options_engine.db

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=8080